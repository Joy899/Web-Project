    <url-pattern>/servlet/updateVipUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addCostInfo</servlet-name>
    <url-pattern>/servlet/addCostInfo</url-pattern>
  </servlet-mapping>
