    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>findCostInfo</servlet-name>
    <servlet-class>servlet.findCostInfo</servlet-class>
  </servlet>









  <servlet-mapping>
    <servlet-name>CartServlet</servlet-name>
    <url-pattern>/servlet/CartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>update</servlet-name>
    <url-pattern>/servlet/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/servlet/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addVipUser</servlet-name>
    <url-pattern>/servlet/addVipUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteVipUser</servlet-name>
    <url-pattern>/servlet/deleteVipUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
