    <servlet-name>addCostInfo</servlet-name>
    <servlet-class>servlet.addCostInfo</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>deleteCostInfo</servlet-name>
    <servlet-class>servlet.deleteCostInfo</servlet-class>
  </servlet>
  <servlet>
