  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>add</servlet-name>
    <servlet-class>servlet.add</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>addVipUser</servlet-name>
    <servlet-class>servlet.addVipUser</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>deleteVipUser</servlet-name>
    <servlet-class>servlet.deleteVipUser</servlet-class>
  </servlet>
